# SonarQube Environment Configuration
# Copy this file to .env and fill in your credentials

# PostgreSQL Database Credentials
POSTGRES_USER=sonar
POSTGRES_PASSWORD=sonar

# SonarQube Admin Token (generate after first login at http://localhost:9000)
# Navigate to: User > My Account > Security > Generate Token
SONAR_TOKEN=your_token_here

# Sonar host (use SonarCloud or local SonarQube)
SONAR_HOST_URL=https://sonarcloud.io

# Sonar organization key for SonarCloud (e.g., clecart)
SONAR_ORGANIZATION=clecart

# GitHub Token for CI/CD (needed for GitHub Actions)
# Create at: https://github.com/settings/tokens (classic token with repo access)
GITHUB_TOKEN=your_github_token_here
